{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/warnings-service/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/warnings-service/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/warnings-service",
  "projects": {
    "warnings-service": {
      "type": "application",
      "root": "apps/warnings-service",
      "entryFile": "main",
      "sourceRoot": "apps/warnings-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/warnings-service/tsconfig.app.json"
      }
    },
    "warnings-service-legacy-adapter": {
      "type": "application",
      "root": "apps/warnings-service-legacy-adapter",
      "entryFile": "main",
      "sourceRoot": "apps/warnings-service-legacy-adapter/src",
      "compilerOptions": {
        "tsConfigPath": "apps/warnings-service-legacy-adapter/tsconfig.app.json"
      }
    },
    "warnings-service-worker": {
      "type": "application",
      "root": "apps/warnings-service-worker",
      "entryFile": "main",
      "sourceRoot": "apps/warnings-service-worker/src",
      "compilerOptions": {
        "tsConfigPath": "apps/warnings-service-worker/tsconfig.app.json"
      }
    }
  }
}